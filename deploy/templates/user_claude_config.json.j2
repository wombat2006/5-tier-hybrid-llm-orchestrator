{
  "version": "1.0",
  "user": "{{ item.name }}",
  "environment": "multi-user-production",
  "api_keys": {
    "anthropic": "{{ shared_api_keys.anthropic }}",
    "openai": "{{ shared_api_keys.openai }}",
    "google": "{{ shared_api_keys.google }}"
  },
  "llm_orchestrator": {
    "url": "{{ llm_orchestrator_url }}",
    "enabled": true,
    "default_tier": 1,
    "max_requests_per_hour": 100
  },
  "security": {
    "api_key_source": "environment",
    "log_requests": true,
    "mask_sensitive_data": true
  },
  "preferences": {
    "default_model": "claude-3-5-sonnet",
    "temperature": 0.7,
    "max_tokens": 4000,
    "auto_save": true
  },
  "workspace": {
    "projects_directory": "~/projects",
    "auto_create_gitignore": true,
    "backup_enabled": true
  },
  "created_at": "{{ ansible_date_time.iso8601 }}",
  "managed_by": "ansible"
}